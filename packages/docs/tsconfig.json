{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "guild": ["./guild/*"]
    },
    "types": ["./auto-imports"]
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.vue", ".vitepress/env.d.ts"],
  "exclude": ["node_modules", "**/dist/**"],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": "true"
}
